<mei xmlns="http://www.music-encoding.org/ns/mei">
    <meiHead>
        <fileDesc>
            <titleStmt>
                <title type="main">Stravinsky</title>
            </titleStmt>
            <pubStmt></pubStmt>
        </fileDesc>
    </meiHead>
    <music>
        <body>
            <mdiv>
                <score>
                    <scoreDef line-order= "11 4 9 2 7 0 5 10 3 8 1 6">
                        <measureGrp>
                            <measureDef n="1" bcount="7"/>
                            <measureDef n="2" bcount="5"/>
                            <measureDef n="3" bcount="6"/>
                            <measureDef n="4" bcount="8"/>
                        </measureGrp>
                    </scoreDef>
                    <section>
                        <measure n="1">
                            <beat n="1">
                                <line n="11" class="" ifBracket="false"/>
                                <line n="4" class="" ifBracket="false"/>
                                <line n="9" class="" ifBracket="false"/>
                                <line n="2" class="" ifBracket="false"/>
                                <line n="7" class="" ifBracket="false"/>
                                <line n="0" class="" ifBracket="false"/>
                                <line n="5" class="" ifBracket="false"/>
                                <line n="10" class="" ifBracket="false"/>
                                <line n="3" class="" ifBracket="false"/>
                                <line n="8" class="" ifBracket="false"/>
                                <line n="1" class="" ifBracket="false"/>
                                <line n="6" class="" ifBracket="false"/>
                            </beat>
                            <setLabel beat="1" place="above"></setLabel>
                            <curvedBrace beat="1" place="right"></curvedBrace>
                            <beat n="2">
                                <line n="11" class="" ifBracket="false"/>
                                <line n="4" class="" ifBracket="false"/>
                                <line n="9" class="" ifBracket="false"/>
                                <line n="2" class="" ifBracket="false"/>
                                <line n="7" class="" ifBracket="false"/>
                                <line n="0" class="" ifBracket="false"/>
                                <line n="5" class="" ifBracket="false"/>
                                <line n="10" class="" ifBracket="false"/>
                                <line n="3" class="" ifBracket="false"/>
                                <line n="8" class="" ifBracket="false"/>
                                <line n="1" class="" ifBracket="false"/>
                                <line n="6" class="" ifBracket="false"/>
                            </beat>
                            <setLabel beat="2" place="above"></setLabel>
                            <curvedBrace beat="2" place="right"></curvedBrace>
                            <!-- other beats from 3 to 7 as 7 bcounts defined above varies from measure to measure-->
                        </measure>
                        <numberBox measure="2"></numberBox>
                        <mmNumber measure="2"></mmNumber>
                        <measureMeta measure="2">
                            <div location="l">Aggregate</div>
                            <div location="r">Octat. III</div>
                            <div location="min"></div>
                        </measureMeta>
                        <!-- other measures  from 2 to 4 as defined avove-->
                    </section>
                </score>
            </mdiv>
        </body>
    </music>
</mei>
